<h1>Movies#index</h1>
<p>Find me in app/views/movies/index.html.erb</p>

    <% @movies.each do |movie| %>
      <%= link_to movie.title, movie_path(movie)%>
      <%= movie.overview %>
      <%= image_tag movie.poster_url, width: '300'%>
      <%= movie.rating %>
      <br>
      <br>
      <% end %>
